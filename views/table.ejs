<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
<!DOCTYPE HTML>
<html>
  <head>
    <title>Explore Supernovas</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

  </head>

  <body>
  <div id="wrapper">
    <table class="table table-condensed">
      <tr>
  			<th>Serial Number</th>
  			<th>Host</th>
  			<th>Galaxy</th>
  			<th>Magnitude</th>
  			<th>Position</th>
  			<th>Type</th>
        <th>Date</th>
  			<th>Discoverer</th>
  		</tr>


      <% _.each(supernovas, function(supernova) { %>
      <tr>
          <td><%= supernova.sn %></td>
          <td><%= supernova.suphost %></td>
          <td><%= supernova.galaxy %></td>
          <td><%= supernova.mag %></td>
          <td><%= supernova.position %></td>
          <td><%= supernova.suptype %></td>
          <td><%= supernova.date %></td>
          <td><%= supernova.discoverer %></td>
      </tr>
      <% }) %>


    </table>
    <ul>




  </ul>
  </div>
  </body>
</html>
